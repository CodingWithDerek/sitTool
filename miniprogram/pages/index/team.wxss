/* pages/index/team.wxss */
.up {
  width: auto;
  height: 90rpx;
  padding: 20rpx;
  background-color: #f2f2f2;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  white-space: nowrap;
}

.up-each {
  /* padding-bottom: 10rpx;
  border-style: none none solid none;
  border-bottom-width: 2px;
  border-color: #f2f2f2 #f2f2f2 #33b5e5 #f2f2f2;
  margin-right: 30rpx;
  color: #33b5e5 */
  margin-right: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #33b5e5;
}

.up-each-line {
  height: 8rpx;
  width: 40rpx;
  background-color: #33b5e5;
  margin-top: 10rpx;
  border-radius: 5rpx;
}

.up-each-noClick {
  margin-right: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: black;
}

.up-each-noClick-line {
  height: 8rpx;
  width: 40rpx;
  background-color: #f2f2f2;
  margin-top: 10rpx;
  border-radius: 5rpx;
}

.up:last-child {
  margin-right: 0;
}
.body{
  background-color: #f2f2f2;
  font-size: 30rpx;
  border: 1px solid #f2f2f2;
}
.body-container{
  margin: 20rpx;
  padding: 20rpx;
  border-radius: 20rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  justify-content: center;
  border: 1px solid white;
}
.body :first-child{
  margin-top: 0rpx;
}
.body-container-row{
  display: flex;
  align-content: center;
}
.body-container-people{
  display: flex;
  align-items: center;
}
.body-container-people-right{
  margin-left: 20rpx;
  border: 1px solid #33b5e5;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 20rpx;
}
.body-container-people-right-content{
  height: 90rpx;
  display: flex;
  align-items: center;
}
.body-container-people-right-content-c2,.body-container-people-right-content-c3{
  margin-left: 20rpx;
}
.body-container-people-right-content-c2 text,.body-container-people-right-content-c3 text{
  padding: 10rpx 20rpx;
  background-color: #f2f2f2;
  border-radius: 10rpx;
  color: #ff1d11;
}
.body-container-people-right-content-c3 text{
  color: #33b5e5;
}
.body-noData{
  margin: 0 20rpx 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
  border-radius: 20rpx;
  height: 100rpx;
}