/* pages/index/team.wxss */
.body{
  background-color: #f2f2f2;
  font-size: 30rpx;
  border: 1px solid #f2f2f2;
}
.body-container{
  margin: 20rpx;
  padding: 20rpx;
  border-radius: 20rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  justify-content: center;
  border: 1px solid white;
}
.body :first-child{
  margin-top: 0rpx;
}
.body-container-row{
  display: flex;
  align-content: center;
}
.body-container-people{
  display: flex;
  align-items: center;
}
.body-container-people-right{
  margin-left: 20rpx;
  border: 1px solid #33b5e5;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 20rpx;
}
.body-container-people-right-content{
  height: 90rpx;
  display: flex;
  align-items: center;
}
.body-container-people-right-content-c2,.body-container-people-right-content-c3{
  margin-left: 20rpx;
}
.body-container-people-right-content-c2 text,.body-container-people-right-content-c3 text{
  padding: 10rpx 20rpx;
  background-color: #f2f2f2;
  border-radius: 10rpx;
  color: #ff1d11;
}
.body-container-people-right-content-c3 text{
  color: #33b5e5;
}
.body-noData{
  margin: 0 20rpx 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
  border-radius: 20rpx;
  height: 100rpx;
}