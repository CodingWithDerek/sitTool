/* pages/sponsor/sponsor.wxss */
.all{
  background-color: #f2f2f2;
}
.container{
  height: 130rpx;
  margin: 20rpx;
  border-radius: 20rpx;
  background-color: white;
  display: flex;
  align-items: center;
  padding: 20rpx;
}
.container-left{
  height: 90rpx;
  width: 90rpx;
  margin-right: 20rpx;
}
.container-left image{
  width: 100%;
  height: 100%;
}
.container-right  {
  height: 90rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between
}
.container-right-up{
  font-size: 35rpx;
}
.container-right-down{
  font-size: 20rpx;
  width: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  border: 1px solid #33b5e5;
  color: #33b5e5
}
.gap{
  height: 1rpx;
}