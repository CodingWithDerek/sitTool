/* pages/person/superManage.wxss */
.all{
  font-size: 30rpx;
  background-color: #f2f2f2;
}
.upBar{
  padding: 20rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding-bottom: 0;
}
.upBar-each_light{
  color: white;
  background-color: #33b5e5;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
}
.upBar-each_dark{
  color: black;
  background-color: white;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
}
.container{
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: white;
  margin: 20rpx;
  border: 1px solid white;
  border-radius: 20rpx;
}
.container-left-each{
  margin: 20rpx;
}
.container-right{
  padding: 5rpx 10rpx;
  color: red;
  border: 1px solid red;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.container-right_agree{
  padding: 5rpx 10rpx;
  color: #05c160;
  border: 1px solid #05c160;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.noDataTip{
  height: 100rpx;
  margin:20rpx;
  border-radius: 20rpx;
  background-color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}
.deleteContainer{
  padding: 20rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  background-color: white;
  margin: 20rpx;
  border-radius: 20rpx;
}
.deleteContainer-right{
  margin-left: 20rpx;
  width: 75%;
}
.buttonArea{
  height: 150rpx;
  background-color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}
.buttonArea-button{
  padding: 5rpx 50rpx;
  font-size: 30rpx;
}